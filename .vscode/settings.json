{
    "cmake.configureArgs": [
        "--graphviz=${workspaceFolder}/docs/graph/graph.dot",
        "--no-warn-unused-cli"
    ],
    "cmake.allowUnsupportedPresetsVersions": true,
    "cmake.useCMakePresets": "always",
    "cmake.ctest.allowParallelJobs": true,
    "[cmake]": {
        "editor.quickSuggestions": {
            "strings": true
        },
        "editor.defaultFormatter": "cheshirekow.cmake-format",
    },
    "cmakeFormat.args": [
        "--config-file",
        "${workspaceFolder}/.cmake-format.yaml"
    ],
    "[yaml]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.autoIndent": "keep",
        "diffEditor.ignoreTrimWhitespace": false,
        "editor.quickSuggestions": {
            "other": true,
            "comments": false,
            "strings": true
        }
    },
    "cmake.configureOnEdit": false,
    "cmake.configureOnOpen": false,
    "files.associations": {
        "*.yml": "yaml",
        "memory": "cpp",
        "charconv": "cpp",
        "chrono": "cpp",
