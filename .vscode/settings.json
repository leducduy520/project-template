  "cmake.configureOnEdit": false,
  "cmake.configureArgs": [
    "--graphviz=docs/graph/graph.dot",
    "--no-warn-unused-cli"
  ],
  "cmake.options.statusBarVisibility": "compact",
  "cmake.configureOnOpen": false,
  "[cmake]": {
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.defaultFormatter": "josetr.cmake-language-support-vscode",
    "editor.formatOnSave": true
  },
  "[yaml]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "keep",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.quickSuggestions": {
      "other": true,
      "comments": false,
      "strings": true
    }
  },
  "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
  "C_Cpp.default.cppStandard": "c++17",
  "C_Cpp.default.cStandard": "c17",
  "[github-actions-workflow]": {
    "editor.indentSize": "tabSize",
    "editor.tabSize": 2,
    "editor.defaultFormatter": "github.vscode-github-actions",
    "editor.quickSuggestions": {
      "other": "on",
      "comments": "off",
      "strings": "off"
    }
  },
  "projectManager.git.baseFolders": ["D:\\"],
  "cmake.parallelJobs": 8,
"intellicodeApiExamples.loggingLevel": "ERROR",
"path-intellisense.autoTriggerNextSuggestion": true,
"C_Cpp.errorSquiggles": "enabled",
"C_Cpp.autocompleteAddParentheses": true
}
