        "xhash": "cpp",
        "xlocbuf": "cpp",
        "xlocinfo": "cpp",
        "xmemory": "cpp",
        "xstring": "cpp",
        "xtr1common": "cpp",
        "xtree": "cpp",
        "*.tcc": "cpp",
        "codecvt": "cpp",
        "cwctype": "cpp",
        "memory_resource": "cpp",
        "string_view": "cpp",
        "numbers": "cpp",
        "cinttypes": "cpp"
    },
    "cmake.clearOutputBeforeBuild": false,
    "cmake.ctest.parallelJobs": 8,
    "git.autofetch": true,
    "git.autofetchPeriod": 180,
    "git.autorefresh": true
}
